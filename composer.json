{
    "name": "wellrested/openapi",
    "description": "OpenAPI documentation generation for WellRESTed APIs",
    "keywords": ["rest", "restful", "api", "openpi", "swagger", "documentation"],
    "homepage": "https://www.wellrested.org",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "PJ Dietz",
            "email": "pjdietz@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "wellrested/wellrested": "^6-dev"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "phpspec/prophecy-phpunit": "^2",
        "phpunit/phpunit": "^9",
        "vimeo/psalm": "^4"
    },
    "autoload": {
        "psr-4": {
            "WellRESTed\\OpenAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WellRESTed\\OpenAPI\\": "tests",
            "WellRESTed\\Test\\": "tests"
        }
    }
}
